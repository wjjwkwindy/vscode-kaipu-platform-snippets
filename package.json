{
    "name": "vscode-kaipu-platform-snippets",
    "publisher": "co2oc",
    "displayName": "vscode kaipu platform snippets",
    "description": "集约化平台代码提示",
    "version": "2.0.0",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/wjjwkwindy/vscode-kaipu-platform-snippets.git"
    },
    "icon": "icon.png",
    "main": "./extension.js",
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.json"
            }
        ],
        "grammars": [
            {
                "path": "./syntaxes/kaipu-tag.json",
                "scopeName": "kaipu-tag.injection",
                "injectTo": [
                    "text.html.derivative"
                ],
                "embeddedLanguages": {
                    "text.html.derivative": "html"
                }
            }
        ],
        "commands": [
            {
                "command": "inject-tag-highlight-to-setting.kaipu",
                "title": "Kaipu: Inject Tag Highlight To Setting"
            }
        ]
    },
    "scripts": {
        "build": "vsce package",
        "lint": "eslint .",
        "pretest": "pnpm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.70.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "eslint": "^8.56.0",
        "typescript": "^5.3.3",
        "@vscode/test-cli": "^0.0.4",
        "@vscode/test-electron": "^2.3.8"
    }
}
